<?xml version="1.0"?>
<configuration>
    <appSettings>
        <add key="DbConnectionStringAZ" value="connection string" />
    </appSettings>
    
    <system.web>
    <authorization>
      <allow roles="group name"/>
      <deny users="*"/>
    </authorization>
    <compilation targetFramework="4.0"/>
      <pages clientIDMode="AutoID" masterPageFile="~/masterpages/mobile.master">
